{
      "name": "zendesk-mcp-server",
      "version": "1.0.0",
      "type": "module",
      "description": "MCP Server for Zendesk API",
      "main": "src/index.js",
      "bin": {
            "zendesk-mcp": "src/index.js"
      },
      "scripts": {
            "start": "node src/index.js",
            "start:http": "node src/http-server.js",
            "dev": "node --watch src/index.js",
            "dev:http": "node --watch src/http-server.js",
            "inspect": "npx -y @modelcontextprotocol/inspector node src/index.js"
      },
      "dependencies": {
            "@modelcontextprotocol/sdk": "^1.24.0",
            "axios": "^1.12.0",
            "dotenv": "^16.3.1",
            "express": "^4.18.2",
            "morgan": "^1.10.0",
            "zod": "^3.22.4"
      }
}